<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wdx.mapper.UserCodeMapper">
    <insert id="addUserCode">
        insert into user_code (username, password,code)  values (#{user.userName},#{user.password},#{code})
    </insert>
</mapper>
